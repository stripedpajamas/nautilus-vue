    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^3.2.0",
    "koa-jwt": "^3.2.2",
    "koa-sslify": "^2.1.2",
    "koa-static": "^4.0.1",
    "koa-websocket": "^4.0.0",
    "mongoose": "^4.11.9",
    "pino": "^4.7.1",
    "vue": "^2.3.4",
    "vue-router": "^2.3.1",
    "vuetify": "^0.15.1",
    "vuex": "^2.4.0",
    "vuex-router-sync": "^4.3.0"
  },
