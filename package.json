{
  "name": "nautilus-vue",
  "version": "1.0.0",
  "description": "A socket-based web app to run PowerShell scripts for Office365",
  "author": "Peter Squicciarini",
  "private": true,
  "scripts": {
    "start": "node server/dist/index.js",
    "build": "cross-env NODE_ENV=production npm run clean && npm run build-vue && npm run build-server",
    "clean": "rimraf server/dist/*.* dist/*.*",
    "build-server": "cross-env BABEL_ENV=server babel server/src --out-dir server/dist",
    "start-server": "npm run build-server && node server/dist/index.js",
    "dev-server": "cross-env BABEL_ENV=server babel-watch server/src/index.js",
    "build-vue": "node build/build.js",
    "start-vue": "node build/dev-server.js",
    "dev-vue": "node build/dev-server.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt": "^1.0.3",
    "duo_web": "git+https://github.com/duosecurity/duo_nodejs.git",
